{
  "name": "@damlys/ts-sdk",
  "version": "0.2.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "Consistent TypeScript SDK for Node.js projects.",
  "keywords": [
    "sdk",
    "typescript"
  ],
  "author": {
    "name": "Damian Łysiak"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/damlys/ts-sdk.git"
  },
  "files": [
    ".eslintrc.json",
    ".prettierrc.json",
    "jest.config.json",
    "project-skeleton",
    "scripts",
    "tsconfig.json"
  ],
  "bin": {
    "add-keepachangelog-header": "scripts/add-keepachangelog-header.js"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.14.4"
  },
  "dependencies": {
    "@damlys/eslint-config": "^1.0.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "version": "node ./scripts/add-keepachangelog-header.js ./CHANGELOG.md && git add ./CHANGELOG.md"
  }
}
